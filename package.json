    "scripts": {
        "test": "yarn graph test",
        "compile": "yarn graph compile",
        "graph:compile": "graph compile",
        "graph:console": "graph console",
        "graph:test": "hardhat test",
        "graph:deploy:testnets": "yarn graph:deploy:ropsten && yarn graph:deploy:goerli && yarn graph:deploy:rinkeby",
        "graph:prepare:local": "mustache networks/local.json subgraph.template.yaml > generated/subgraph.yaml",
        "graph:compile:local": "rm -rf generated/ && mkdir generated/ && yarn graph:prepare:local && graph codegen --output-dir generated/ generated/subgraph.yaml && graph build generated/subgraph.yaml --output-dir generated/build",
